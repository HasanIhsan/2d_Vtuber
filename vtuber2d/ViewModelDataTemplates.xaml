<!-- ViewModelDataTemplates.xaml -->
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:vtuber2d.Screens"
                    xmlns:viewmodels="clr-namespace:vtuber2d.viewmodels">

    <DataTemplate DataType="{x:Type viewmodels:SetupScreenViewModel}">
        <views:SetupScreen />
    </DataTemplate>

  <DataTemplate DataType="{x:Type viewmodels:MainScreenViewModel}">
    <views:MainScreen/>
  </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodels:SelectScreenViewModel}">
        <views:SelectScreen/>
    </DataTemplate>

	<DataTemplate DataType="{x:Type viewmodels:FaceModelScreenViewModel}">
		<views:faceModelScreen/>
	</DataTemplate>

</ResourceDictionary>